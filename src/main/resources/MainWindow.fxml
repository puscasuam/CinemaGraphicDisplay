<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox alignment="center" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MainController">
<VBox spacing="20">
    <Label text="Options:" />
    <Button fx:id="btnMovieCRUD" alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" onAction="#btnMovieCRUDClick" text="Movie CRUD" textAlignment="JUSTIFY" textFill="#171616" />
    <Button fx:id="btnClientCRUD" maxWidth="1.7976931348623157E308" onAction="#btnClientCRUDClick" text="Client CRUD" />
    <Button fx:id="btnReservationCRUD" maxWidth="1.7976931348623157E308" onAction="#btnReservationCRUDClick" text="Reservation CRUD" />
    <Button fx:id="btnFullTextSearch" maxWidth="1.7976931348623157E308" onAction="#btnFullTextSearchClick" text="Search in movie/client fields" />
    <Button fx:id="btnReservationsTimeFrame" maxWidth="1.7976931348623157E308" onAction="#btnReservationsTimeFrameClick" text="Show reservations time frame report" />
    <Button fx:id="btnMoviesOrderedByBookings" maxWidth="1.7976931348623157E308" onAction="#btnShowMoviesOrderedByBookings" text="Show movies ordered by bookings" />
    <Button fx:id="btnClientsOrderedByFidelityPoints" maxWidth="1.7976931348623157E308" onAction="#btnClientsOrderedByFidelityPointsClick" text="Show client cards sorted by fidelity points" />
    <Button fx:id="btnDeleteReservationBetweenTwoDates" maxWidth="1.7976931348623157E308" onAction="#btnDeleteReservationBetweenTwoDatesClick" text="Delete all reservations between two dates" />
    <Button fx:id="btnUpdateFidelityPoints" maxWidth="1.7976931348623157E308" onAction="#btnUpdateFidelityPointsClick" text="Update fidelity points" />

</VBox>


</HBox>
